#include <iostream>

using namespace std;

int main()
{
		srand(time(NULL));
		setlocale(LC_ALL, "RUS");
		cout << "ЗАДАЙТЕ МАССИВ" << endl;
		int n, Elenushka, x;
		cin >> n;

		int* MAS = new int[n];
	
	for (int i = 0; i < n; ++i)
		MAS[i] = i + 1;
	for (int i = 0; i < n; ++i) {
		Elenushka = MAS[i];
		x = rand() % n;
		MAS[i] = MAS[x];
		MAS[x] = Elenushka;

	}

	for (int i = 0; i < n; ++i)
		cout << MAS[i] << ' ';
	return 0;
}
